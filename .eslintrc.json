{
  "extends": [
    "next/core-web-vitals",
    "airbnb",
    "airbnb-typescript",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "project": "./tsconfig.json",
    "extraFileExtensions": [".css", ".scss"]
  },
  "rules": {
    "react/react-in-jsx-scope": "off",
    "endOfLine": 0,
    "react/no-unescaped-entities": 0,
    "no-alert": 0
  }
}
